const {
    unmapAllExcept,
} = api;
settings.tabsMRUOrder = false;
settings.hintExplicit = true;
settings.stealFocusOnLoad = false;
settings.digitForRepeat = false;
settings.tabsThreshold = 50;
unmapAllExcept(['f','t','T']);
map('<Ctrl-Alt-l>', 'f');
map('<Ctrl-Alt-t>', 't');
map('<Ctrl-Alt-y>', 'T');
unmap('f');
unmap('t');
unmap('T');
